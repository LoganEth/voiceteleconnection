# Telegram Bot Token from BotFather
TELEGRAM_TOKEN=your_telegram_token_here

# Voiceflow API Key
VOICEFLOW_API_KEY=your_voiceflow_api_key_here

# Voiceflow Version ID (optional, defaults to 'production')
# Use 'production' for published version or 'development' for testing
VOICEFLOW_VERSION_ID=production

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
